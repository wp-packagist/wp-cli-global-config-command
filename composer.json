{
  "name": "wp-packagist/wp-cli-global-config-command",
  "description": "WP-CLI Global Configuration Command.",
  "type": "wp-cli-package",
  "homepage": "https://github.com/wp-packagist/wp-cli-global-config-command",
  "license": "MIT",
  "keywords": [
    "cli",
    "wordpress",
    "wp-cli"
  ],
  "authors": [
    {
      "name": "Mehrshad Darzi",
      "email": "mehrshad198@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "": "src/"
    },
    "files": [
      "command.php"
    ]
  },
  "require": {
    "php": "^5.6 || ^7.0 || ^8.0",
    "wp-packagist/wp-cli-helper": ">=1.0.2",
    "ext-json": "*"
  },
  "require-dev": {
    "wp-cli/wp-cli": "^2.0",
    "phpunit/phpunit": "^4.8"
  },
  "extra": {
    "commands": [
      "global-config",
      "global-config set",
      "global-config remove",
      "global-config reset",
      "global-config list",
      "global-config get"
    ]
  },
  "support": {
    "issues": "https://github.com/wp-packagist/wp-cli-global-config-command/issues"
  },
  "scripts": {
    "test": "phpunit"
  }
}
